{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "arhitector/yandex": "^1.2",
        "aspose-cloud/aspose-words-cloud": "^23.10",
        "coolsam/flatpickr": "^5.0",
        "doctrine/dbal": "3.6.6",
        "dompdf/dompdf": "^2.0",
        "filament/actions": "^4.0",
        "filament/filament": "^4.0",
        "filament/forms": "^4.0",
        "filament/infolists": "^4.3",
        "filament/notifications": "^4.3",
        "filament/tables": "^4.0",
        "filament/widgets": "^4.3",
        "guzzlehttp/guzzle": "^7.2",
        "hflabs/dadata": "^22.3",
        "laravel/framework": "^12.0",
        "laravel/sanctum": "4.x-dev",
        "laravel/telescope": "^5.16",
        "laravel/tinker": "^2.8",
        "maatwebsite/excel": "^3.1",
        "novadaemon/filament-pretty-json": "^3.1",
        "phpoffice/phpword": "^1.1",
        "phpseclib/phpseclib": "^3",
        "rappasoft/laravel-authentication-log": "^5.0",
        "shuvroroy/filament-spatie-laravel-backup": "^3.2",
        "stechstudio/filament-impersonate": "4.0.1",
        "tapp/filament-authentication-log": "^5.0",
        "ufee/amoapi": "0.9.3.5",
        "vgrish/yclients-sdk-php": "^1.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "filament/upgrade": "^4.0",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        },
        "audit": {
            "block-insecure": false
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
